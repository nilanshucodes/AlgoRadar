{
    "version":2,
    "builds": [
        {
            "src":"./index.py",
            "use": "@vercel/python",
            "config":{"runtime":"python3.8"}
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "crons": [
        {
        "path": "/api/cron/refresh-contests",
        "schedule": "*/10 * * * *"
      }
    ]
}